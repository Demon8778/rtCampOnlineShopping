		"babel-loader": "^8.0.6",
		"css-loader": "^2.1.1",
		"isomorphic-style-loader": "^5.0.1",
		"jest": "^24.8.0",
		"mini-css-extract-plugin": "^0.6.0",
		"node-sass": "^4.12.0",
		"nodemon": "^1.19.0",
